{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "location": {
            "value": "westeurope"
        },
        "suffix": {
            "value": "app01-d-we-01"
        },
        "kubernetesVersion": {
            "value": "1.28.5"
        },
        "vmSize": {
            "value": "Standard_B2ms"
        },
        "adminGroupObjectIDs": {
            "value": [
                "00000000-0000-0000-0000-000000000000"
            ]
        },
        "fluxGitRepository": {
            "value": "https://github.com/jacqinthebox/kubeflow-demo"
        },
        "spokeAddressSpace": {
            "value": "10.200.0.0/16"
        },
        "enablePrivateCluster": {
            "value": true
        },
        "disableLocalAccounts": {
            "value": true
        },
        "fluxGitBranch": {
            "value": "main"
        },
        "spokeSubnets": {
            "value": [
                {
                    "name": "sn-mgm",
                    "addressPrefix": "10.200.0.0/24"
                },
                {
                    "name": "sn-apps",
                    "addressPrefix": "10.200.1.0/24"
                },
                {
                    "name": "sn-kube",
                    "addressPrefix": "10.200.32.0/19"
                }
            ]
        },
        "kustomizations": {
            "value": {
                "base": {
                    "dependsOn": [],
                    "path": "./gitops/infra",
                    "prune": true,
                    "syncIntervalInSeconds": 300,
                    "timeoutInSeconds": 180
                },
                "apps": {
                    "dependsOn": [],
                    "path": "./gitops/apps",
                    "prune": true,
                    "syncIntervalInSeconds": 300,
                    "timeoutInSeconds": 180
                }
            }
        }
    }
}